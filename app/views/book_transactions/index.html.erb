<p id="notice"><%= notice %></p>
<%if @book_transactions.empty? %>
  <p>There are no books.</p>
<%end%>
<h1>Listing Book Transactions</h1>
<table class="table table-striped">
  <thead>
    <tr>
      <th>Isbn</th>
      <th>Member</th>
      <th>Timestamp</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @book_transactions.each do |book_transaction| %>
      <tr>
        <td><%= book_transaction.book_id %></td>
        <td><%= book_transaction.member_id %></td>
        <td><%= book_transaction.created_at %></td>
        <td><%= link_to 'Show', book_transaction %></td>
        <td><%= link_to 'Edit', edit_book_transaction_path(book_transaction) %></td>
        <td><%= link_to 'Destroy', book_transaction, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Book transaction', new_book_transaction_path %>
