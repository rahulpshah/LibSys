<p>Member History</p>
<%books = @member.get_history %>
<table class="table table-striped">
  <thead>
    <tr>
      <th>Book Name</th>
      <th>Action</th>
      <th>Date</th>
    </tr>
  </thead>

  <tbody>
    <% books.each do |book| %>
      <%unless book.first[0].nil? %>
        <tr>   
          <td><%= book.first[0].isbn %></td>
          <td><%= book.first[1] %></td>
          <td><%= book.first[0].created_at %></td>
      <%end%>
        </tr>
    <%end%>
  </tbody>
</table>
